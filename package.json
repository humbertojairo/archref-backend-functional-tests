{
  "name": "cypress-cucumber-template",
  "version": "1.0.1",
  "description": "Template Cucumber Cypress",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "test": "cypress run -e TAGS='@feature_user' --browser chrome",
    "cucumber": "cypress-tags run -e"
  },
  "dependencies": {
    "cucumber-html-reporter": "5.5.0",
    "cypress-cucumber-preprocessor": "4.3.1",
    "cypress-plugin-api": "^2.11.1",
    "cypress-plugin-tab": "1.0.5",
    "cypress-xpath": "1.6.2",
    "fs": "0.0.2",
    "fs-extra": "10.1.0",
    "jsonschema": "1.4.1",
    "mocha": "10.0.0",
    "mochawesome": "7.1.3",
    "mochawesome-merge": "4.2.1",
    "multiple-cucumber-html-reporter": "1.20.0"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "integrationFolder": "cypress/integration/specs/",
    "step_definitions": "cypress/integration/step_definitions/",
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    }
  },
  "devDependencies": {
    "cypress": "^9.5.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
